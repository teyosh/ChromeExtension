{
  "manifest_version": 2,
  "name": "Stickers",
  "version": "0.2.1",
  "description": "Stickers APP",
  "author": "Keiko Tsushima",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon38.png"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "css": ["content.css"],
    "js": ["jquery-2.1.0.min.js", "jquery-ui.js", "content.js"]
  }]
}
